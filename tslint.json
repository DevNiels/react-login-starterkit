{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "object-literal-sort-keys": false,
    "quotemark": [true, "single", "jsx-single", "avoid-escape"],
    // "no-console": false,
    "jsx-no-lambda": false
  }
}
